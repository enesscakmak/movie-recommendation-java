<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.main.Register">
   <children>
      <TextField fx:id="Username" layoutX="30.0" layoutY="85.0" prefHeight="37.0" prefWidth="259.0" promptText="Username" style="-fx-border-radius: 50; -fx-background-radius: 30; -fx-border-color: #000000;" />
      <Label layoutX="30.0" layoutY="58.0" text="Username">
         <font>
            <Font name="Cambria Math" size="14.0" />
         </font>
      </Label>
      <Label layoutX="30.0" layoutY="150.0" text="Password">
         <font>
            <Font name="Cambria Math" size="14.0" />
         </font>
      </Label>
      <Button fx:id="Register" layoutX="267.0" layoutY="243.0" mnemonicParsing="false" onAction="#registerUser" prefHeight="30.0" prefWidth="85.0" text="Register">
         <font>
            <Font name="Cambria Math" size="15.0" />
         </font>
      </Button>
      <Hyperlink fx:id="HaveAccount" layoutX="229.0" layoutY="288.0" onAction="#exit" text="Already have an account?">
         <font>
            <Font name="Cambria Math" size="14.0" />
         </font>
      </Hyperlink>
      <Hyperlink fx:id="Login" layoutX="288.0" layoutY="310.0" onAction="#exit" text="Login">
         <font>
            <Font name="Cambria Math" size="14.0" />
         </font>
      </Hyperlink>
      <TextField fx:id="Password" layoutX="30.0" layoutY="172.0" prefHeight="37.0" prefWidth="259.0" promptText="Password" style="-fx-border-radius: 50; -fx-background-radius: 30; -fx-border-color: #000000;" />
      <Label fx:id="warning" layoutX="169.0" layoutY="347.0" prefHeight="38.0" prefWidth="282.0" />
      <TextField fx:id="Name" layoutX="311.0" layoutY="85.0" prefHeight="37.0" prefWidth="259.0" promptText="Name" style="-fx-border-radius: 50; -fx-background-radius: 30; -fx-border-color: #000000;" />
      <TextField fx:id="Surname" layoutX="311.0" layoutY="172.0" prefHeight="37.0" prefWidth="259.0" promptText="Surname" style="-fx-border-radius: 50; -fx-background-radius: 30; -fx-border-color: #000000;" />
      <Label layoutX="311.0" layoutY="58.0" text="Name">
         <font>
            <Font name="Cambria Math" size="14.0" />
         </font>
      </Label>
      <Label layoutX="311.0" layoutY="150.0" text="Surname">
         <font>
            <Font name="Cambria Math" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
