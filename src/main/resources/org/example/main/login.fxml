<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@login_register.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.main.MainController">
   <children>
      <TextField fx:id="Username" layoutX="175.0" layoutY="73.0" prefHeight="42.0" prefWidth="250.0" promptText="Username" style="-fx-border-radius: 50; -fx-background-radius: 30; -fx-border-color: #000000;" styleClass="text-field" stylesheets="@login_register.css" />
      <Label layoutX="185.0" layoutY="47.0" stylesheets="@login_register.css" text="Username">
         <font>
            <Font name="Cambria Math" size="14.0" />
         </font>
      </Label>
      <Label layoutX="187.0" layoutY="135.0" text="Password">
         <font>
            <Font name="Cambria Math" size="14.0" />
         </font>
      </Label>
      <Hyperlink fx:id="DontHaveAccount" alignment="CENTER" contentDisplay="CENTER" layoutX="223.0" layoutY="286.0" onAction="#register" text="Don't have an account?">
         <font>
            <Font name="Cambria Math" size="14.0" />
         </font>
      </Hyperlink>
      <Hyperlink fx:id="Register" alignment="CENTER" contentDisplay="CENTER" layoutX="270.0" layoutY="312.0" onAction="#register" text="Register">
         <font>
            <Font name="Cambria Math" size="14.0" />
         </font>
      </Hyperlink>
      <Label fx:id="warning" alignment="CENTER" contentDisplay="CENTER" layoutX="159.0" layoutY="338.0" prefHeight="38.0" prefWidth="282.0" />
      <PasswordField fx:id="Password" layoutX="175.0" layoutY="161.0" prefHeight="38.0" prefWidth="259.0" promptText="Password" style="-fx-background-radius: 30; -fx-border-radius: 50; -fx-border-color: #000000;" styleClass="back" />
      <Button fx:id="Login" alignment="CENTER" contentDisplay="CENTER" layoutX="240.0" layoutY="238.0" mnemonicParsing="false" onAction="#login" prefHeight="30.0" prefWidth="85.0" text="Log In">
         <font>
            <Font name="Cambria Math" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
